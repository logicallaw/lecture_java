interface USB_4{
	void readUSB();
	default void writeUSB() {
		System.out.println("Can't write to USB");
	}
}

interface USBA_4 implements USB_4{
	void connectA();
}
interface USBC_4 implements USB_4{
	void connectC();
}


public class Lab05_4 {
	public static void main(String[] args) {
		S22_4 s22 = new S22_4("S22");
		MP3_4 mp3 = new MP3_4("MP3");
		
		USB_4[] hub = new USB_4[] {s22, mp3};
		for (USB_4 u : hub) {
			connect(u);
			readUSB(u);
			writeUSB(u);
		}
	}
}

